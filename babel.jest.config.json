{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": "defaults",
                "modules": false
            }
        ],
        [
            "@babel/preset-react",
            {
                "runtime": "automatic",
                "importSource": "react"
            }
        ]
    ],
    "plugins": ["@babel/plugin-transform-modules-commonjs"]
}
